@model MediaGraph.ViewModels.Edit.CompanyNodeViewModel
<div class="col s12 input-field">
    @Html.TextBoxFor(x => x.CommonName, new { @class = "validate" })
    @Html.LabelFor(x => x.CommonName)
</div>
<div class="col s12 input-field">
    <label for="other-name-chips" class="active">Other Names</label>
    <div id="other-name-chips" class="chips"></div>
</div>
<div class="col s12 m6 input-field">
    <input id="@Html.NameFor(x => x.ReleaseDate)" type="date" name="@Html.NameFor(x => x.ReleaseDate)" class="validate" value="@MediaGraph.Code.DateValueConverter.ToDisplayFormat(Model.ReleaseDate)" />
    <label for="@Html.NameFor(x => x.ReleaseDate)" class="active">Date Founded</label>
</div>
<div class="col s12 m6 input-field">
    <input id="@Html.NameFor(x => x.DeathDate)" type="date" name="@Html.NameFor(x => x.DeathDate)" class="validate" value="@MediaGraph.Code.DateValueConverter.ToDisplayFormat(Model.DeathDate)" />
    <label for="@Html.NameFor(x => x.DeathDate)" class="active">Date Defunct</label>
</div>
